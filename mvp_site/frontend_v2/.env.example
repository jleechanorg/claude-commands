# Frontend v2 Environment Variables
# Copy this file to .env.local for local development

# API Configuration
VITE_API_BASE_URL=http://localhost:8081

# Test Mode Configuration (Development Only)
# WARNING: These variables only work in development mode (npm run dev)
# They are automatically disabled in production builds
VITE_TEST_MODE=false
VITE_TEST_USER_ID=test-user-123

# Feature Flags
VITE_ENABLE_MOCK_MODE=true
VITE_ENABLE_DEBUG_LOGGING=false
